      "source": [
        "# **Selection Sort**"
      ],
      "metadata": {
        "id": "s9A07OUWwpLw"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "3k36Pv3wm9Mi"
      },
      "outputs": [],
      "source": [
        "def selection(a):\n",
        "  for i in range(0, len(a) - 1):\n",
        "    i_min = i\n",
        "    for j in range(i + 1, len(a)):\n",
        "      if a[j] < a[i_min]:\n",
        "        i_min = j\n",
        "    \n",
        "    temp = a[i]\n",
        "    a[i] = a[i_min]\n",
        "    a[i_min] = temp\n",
        "  \n",
        "  return a"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "selection([1,4,0,2,9,2])"
      ],
