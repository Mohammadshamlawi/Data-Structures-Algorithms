        "def hash(key, n):\n",
        "  val = 0\n",
        "  for i in key:\n",
        "    val = val + ord(i)\n",
        "  return val % n\n",
        "\n",
        "def insert(key, n):\n",
        "  index = 0\n",
        "  for i in key:\n",
        "    index = index + ord(i)\n",
        "  \n",
        "  items = [None] * n\n",
        "  items[index % n] = key\n",
        "  return items"
      ],
